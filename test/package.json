{
  "name": "bedrock-rsvp-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "@digitalbazaar/http-client": "^1.0.0",
    "bedrock": "^3.1.1",
    "bedrock-express": "^3.1.0",
    "bedrock-https-agent": "^1.1.0",
    "bedrock-mongodb": "^7.1.0",
    "bedrock-rsvp": "file:..",
    "bedrock-server": "^2.4.1",
    "bedrock-test": "^5.3.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-rsvp/**"
    ],
    "exclude": [
      "node_modules/bedrock-rsvp/node_modules/**"
    ]
  }
}
